{
  "name": "typeorm-cursor-connection",
  "version": "0.5.2",
  "description": "Relay Cursor Connection implementations for TypeORM",
  "author": "Max Choi <hanpama@gmail.com>",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "tags": [
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanpama/typeorm-cursor-connection.git"
  },
  "bugs": {
    "url": "https://github.com/hanpama/typeorm-cursor-connection/issues"
  },
  "homepage": "https://github.com/hanpama/typeorm-cursor-connection",
  "license": "MIT",
  "peerDependencies": {
    "typeorm": "^0.2.9"
  },
  "dependencies": {
    "@girin/connection": "^0.5.2-alpha.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "mongodb": "^3.1.10",
    "mysql": "^2.16.0",
    "pg": "^7.6.1",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.9",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "codecov": "^3.1.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublish": "npm run build",
    "lint": "tslint --project tslint.json src/**/*.ts",
    "test": "jest --verbose",
    "test:ci": "npm run coverage -- --ci",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "tests/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
